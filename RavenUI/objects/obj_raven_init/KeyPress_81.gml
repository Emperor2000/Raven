/// @description Change Theme button
// You can write your code in this editor


if (THEME == THEME.RAVEN) {
	THEME = THEME.DARK;
} else if (THEME == THEME.DARK) {
	THEME = THEME.LIGHT;
} else if (THEME == THEME.LIGHT) {
	THEME = THEME.APPLE;	
} else if (THEME == THEME.APPLE) {
	THEME = THEME.COFFEE;	
} else if (THEME == THEME.COFFEE) {
	THEME = THEME.RAVEN;	
}